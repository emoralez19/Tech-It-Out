<!-- stylization for the tables as well as having each table side by side -->
    <style>
    *{
        box-sizing: border-box;
      }
      
      .row {
        margin-left:-5px;
        margin-right:-5px;
      }
        
      .column {
        float: left;
        width: 50%;
        padding: 15px;
      }
      
      /* Clearfix (clear floats) */
        .row::after {
        content: "";
        clear: both;
        display: table;
      } 
      
      /* table settings */
      table {
        border-collapse: collapse; 
        border-spacing: 0; 
        width: 100%;
        border: 2px solid #F0E68C;
      }
      
      /* table header settings */
      th {
        text-align: left;
        padding: 10px;
      }
      
      /* table data settings */
      td {
        text-align: left;
        padding: 10px; 
      }
      
      /* sets the even rows background colour set to light sky blue */
       tr:nth-child(even){
        background-color: #87CEFA;
      }

      /* sets the odd rows background colour set to white */
      tr:nth-child(odd)
      {
        background-color: white;
      }

      /* every even row the background colour is set to this gold colour */
      tr:first-child {
        background-color: #B8860B;
      }

      /* every even row the background colour is set to this light gold colour */
      tr:nth-child(2){
        background-color: #F0E68C;
      }
      
      

      /* mobile view for the tables if the screen is lower than 600 pixels wide then the
         tables are placed on top of one another */
@media screen and (max-width: 600px) {
    .column {
      width: 100%;
    }
  }
      </style>


